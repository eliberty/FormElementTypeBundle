{% block eliberty_boolean_stylesheet %}
{% spaceless %}
    {{formelementtypemode}}
    {% if formelementtypemode is defined and formelementtypemode == 'redpill' %}
        {% stylesheets '@RedpillBundle/Resources/public/css/bool_form.css' %}
            <link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="screen" />
        {% endstylesheets %}
    {% else %}
        {% stylesheets '@ElibertyFormElementTypeBundle/Resources/public/css/bool_form.css' %}
            <link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="screen" />
        {% endstylesheets %}
    {% endif %}
{% endspaceless %}
{% endblock eliberty_boolean_stylesheet %}

