{
    "name": "eliberty/formelementtype-bundle",
    "type": "symfony-bundle",
    "description": "Extra form types for your Symfony2 projects",
    "keywords": ["form", "extra form", "eliberty"],
    "license": "PROPRIETARY",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "philippe vesin",
            "email": "pvesin@eliberty.fr"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/eliberty/GenemuFormBundle.git"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "genemu/form-bundle": "~2.3",
        "symfony/config": ">=2.1.0||~3.0||4.0",
        "symfony/dependency-injection": "~2.1||~3.0||~4.0",
        "symfony/http-kernel": "~2.1||~3.0||4.0",
        "justinrainbow/json-schema": "~1.6"
    },
    "extra": {
        "branch-alias":{
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": { "Eliberty\\Bundle\\FormElementTypeBundle\\": "" }
    }
}
